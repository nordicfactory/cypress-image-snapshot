{
    "name": "@bannerflow/cypress-image-snapshot",
    "version": "1.1.4",
    "description": "Cypress bindings for jest-image-snapshot.",
    "repository": "https://github.com/nordicfactory/cypress-image-snapshot",
    "author": "Jack Cross <jack@palmer.net>",
    "license": "MIT",
    "files": [
        "*.js",
        "*.d.ts"
    ],
    "types": "./index.d.ts",
    "engines": {
        "node": ">=8"
    },
    "scripts": {
        "prebuild": "rimraf dist",
        "build": "babel src --out-dir dist && npx copyfiles package.json dist",
        "test": "jest",
        "lint": "eslint src",
        "prepublish": "npm run lint && npm run test && npm run build"
    },
    "dependencies": {
        "chalk": "^2.4.1",
        "fs-extra": "^7.0.1",
        "glob": "^7.1.3",
        "jest-image-snapshot": "6.1.0",
        "pkg-dir": "^3.0.0",
        "term-img": "^4.0.0"
    },
    "devDependencies": {
        "@changesets/changelog-github": "^0.2.8",
        "@changesets/cli": "^2.13.1",
        "babel-cli": "^6.26.0",
        "babel-plugin-transform-object-rest-spread": "^6.26.0",
        "babel-preset-env": "^1.7.0",
        "copyfiles": "^2.4.1",
        "eslint": "^5.10.0",
        "eslint-config-airbnb-base": "^13.1.0",
        "eslint-config-prettier": "^3.3.0",
        "eslint-plugin-cypress": "^2.1.3",
        "eslint-plugin-import": "^2.14.0",
        "husky": "^1.2.1",
        "jest": "^28.1.3",
        "prettier": "1.15.3",
        "rimraf": "^2.6.2"
    },
    "peerDependencies": {
        "cypress": "^10.0.3"
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run lint"
        }
    },
    "prettier": {
        "printWidth": 80,
        "semi": true,
        "singleQuote": true,
        "trailingComma": "es5"
    },
    "eslintConfig": {
        "extends": [
            "airbnb-base",
            "prettier"
        ],
        "plugins": [
            "cypress"
        ],
        "env": {
            "cypress/globals": true
        }
    },
    "babel": {
        "presets": [
            [
                "env",
                {
                    "targets": {
                        "node": "8.0"
                    }
                }
            ]
        ],
        "plugins": [
            "transform-object-rest-spread"
        ]
    },
    "jest": {
        "testPathIgnorePatterns": [
            "<rootDir>/examples/",
            "<rootDir>/node_modules/"
        ]
    },
    "publishConfig": {
        "access": "restricted"
    }
}
